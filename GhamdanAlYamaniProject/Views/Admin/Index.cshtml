@model IEnumerable<Admin>
<label  class="control-label">To Add New Admin --></label>
<a asp-action="Create"  class="btn btn-primary" >Add New</a>
<br>
<table class="table">
	<titel>  Welcome to View Data Admins Page --><hr></titel>
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Email</th>
			<th>Password</th>
			<th>Action</th>
		</tr>
	</thead>
		<tbody>
	@foreach(var e in Model)
	{
		<tr>
			<td>@e.A_Id</td>
			<td>@e.A_Name</td>
			<td>@e.A_Email</td>
			<td>@e.A_Password</td>
			<td>
				<a asp-action="Edit" asp-route-id="@e.A_Id">Edit</a>
				<a asp-action="Delete" asp-route-id="@e.A_Id">Delete</a>

			</td>
		</tr>
	}		
	</tbody>
</table>
